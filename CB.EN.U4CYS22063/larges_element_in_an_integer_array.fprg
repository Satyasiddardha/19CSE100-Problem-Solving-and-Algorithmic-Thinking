    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="declare integer i and max"/>
            <declare name="i, max" type="Integer" array="False" size=""/>
            <declare name="a" type="Integer" array="True" size="5"/>
            <comment text="enter numbers"/>
            <output expression="&quot;Enter 5 numbers&quot;" newline="True"/>
            <for variable="i" start="0" end="4" direction="inc" step="1">
                <input variable="a[i]"/>
