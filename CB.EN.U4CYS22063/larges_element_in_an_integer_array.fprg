                    <then>
                        <assign variable="max" expression="a[i]"/>
                    </then>
                    <else/>
                </if>
            </for>
            <comment text="output will be displayed here"/>
            <output expression="&quot;The greatest number is&quot; &amp;max" newline="True"/>
        </body>
